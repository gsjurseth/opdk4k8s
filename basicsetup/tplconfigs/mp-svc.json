{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "mphs",
    "namespace": "apigee"
  },
  "spec": {
    "clusterIP": "None",
    "ports": [
      {
        "name": "jmx",
        "port": 1101
      },
      {
        "name": "dist-cache",
        "port": 4528
      },
      {
        "name": "mgmt-port",
        "port": 8082
      },
      {
        "name": "tls-for-mp",
        "port": 8443
      },
      {
        "name": "r-mp-port",
        "port": 8998
      }
    ],
    "selector": {
      "app": "mp"
    }
  }
}
